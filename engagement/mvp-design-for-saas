  --select-field-padding-right: 12px; /* Match the padding values for your form fields */
  --list-padding-top-bottom: 15; /* This is the top and bottom padding for the dropdown list */
  --list-max-height: auto;
  --list-min-width: 100%; /* You can set this to "Auto" as well */
  --list-item-padding-left: 18px; 
  --list-item-padding-right: 18px;
  --list-item-min-height: 50px; /* This is the height of each list item in the dropdown */
  --list-item-line-height: 50px; /* This is the height of each list item in the dropdown */
  						
           /* ^^^  Make sure both "line-height" and "min-height" are the same! ^^^  */
}




.nice-select {
  -webkit-tap-highlight-color: transparent;
  box-sizing: border-box;
  clear: both;
  padding-left: var(--select-field-padding-left);
  padding-right: var(--select-field-padding-right);
  position: relative;
  text-align: left !important;
