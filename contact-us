<script>
$(document).ready(function () {
  const b = document.querySelector('html');
  b.setAttribute('itemtype', 'https://schema.org/FAQPage');
  b.setAttribute('itemscope', 'itemscope');

  const match = document.cookie.match('(?:^|;)\\s*_ga=([^;]*)');
  const raw = match ? decodeURIComponent(match[1]) : null;
  const input = document.getElementById('customerid');
  let gacid = null;

  if (raw) {
    const gacidMatch = raw.match(/(\d+\.\d+)$/);
    gacid = gacidMatch ? gacidMatch[1] : null;
  }

  if (gacid) {
    input.value = gacid;
    input.setAttribute('value', gacid);
    input.setAttribute('readonly', '');
  }

  $('select').niceSelect();

  function initializeFormValidation() {
    const formLoadedTime = Date.now();

    $.validator.addMethod("noHTML", function (value, element) {
      const htmlTagRegex = /<[a-z][\s\S]*>/i;
      const aTagRegex = /<a[\s\S]*>|\ba href\b/i;
      return !htmlTagRegex.test(value) && !aTagRegex.test(value);
    }, "code is not allowed");


    $.validator.addMethod("honeypot", function (value, element) {
      return value === "";
    }, "Bot submission detected.");


    $.validator.addMethod("timeCheck", function (value, element) {
      const currentTime = Date.now();
      const timeSpent = currentTime - formLoadedTime;
      return timeSpent >= 3000;
    }, "Form submitted too quickly. Possible bot activity.");


    $.validator.addMethod("notDefault", function (value, element) {
      const selectedText = $('.nice-select .current').text();
      return selectedText !== "-";
    }, "Please select how you heard about us");


    $("form").validate({
      rules: {
        FullName: {
          required: true,
          maxlength: 256
        },
        Company: {
          required: true,
          maxlength: 256
        },
        "E-mail": {
          required: true,
          email: true,
          maxlength: 256
        },
        Description: {
          required: true,
          maxlength: 1000,
          noHTML: true
        },
        "Came-from": {
          required: true,
          notDefault: true
        },
        honeypot: {
          honeypot: true
        },
        timeCheck: {
          timeCheck: true
        }
      },
      messages: {
        FullName: {
          required: "Please enter your full name",
          maxlength: "Your name must be no longer than 256 characters"
        },
        Company: {
          required: "Please enter your company name",
          maxlength: "Your company name must be no longer than 256 characters"
        },
        "E-mail": {
          required: "Please enter your email",
          email: "Please enter a valid email address",
          maxlength: "Your email must be no longer than 256 characters"
        },
        Description: {
          required: "Please describe how we can help you",
          maxlength: "Your description must be no longer than 1000 characters",
          noHTML: "HTML tags are not allowed"
        },
        "Came-from": {
          required: "Please select how you heard about us",
          notDefault: "Please select how you heard about us"
        }
      },
      errorPlacement: function (error, element) {
        if (element.attr("name") == "Came-from") {
          error.insertAfter(".howdid-2 .nice-select");
        } else {
          error.insertAfter(element);
        }
      }
    });

    const submit = document.querySelector("input[type='submit'][value='Send']");
    const honeypot = document.querySelector("#honeypot");

    honeypot.oninput = function () {
      if (honeypot.value.length > 0) {
        submit.disabled = true;
      }
    };

    $('#Description').on('input', function () {
      const currentLength = $(this).val().length;
      const errorLabel = $('#Description-error');

      if (currentLength >= 1000) {
        errorLabel.text('You have reached 1000 characters in your description.');
        errorLabel.show();
      } else {
        errorLabel.hide();
      }
    });
  }


  initializeFormValidation();
});

$(document).ready(function () {
  $('select').niceSelect();
});
</script>

<script>
  const swiper = new Swiper('.slider-container', {
    direction: 'vertical',
    slidesPerView: 3,
    roundLengths: true,
    spaceBetween: 16,
    loop: true,
    autoplay: {
      delay: 5000,
      reverseDirection: true,
      disableOnInteraction: false,
      pauseOnMouseEnter: true,
    },
    on: {
      slideChange: function () {
        const slides = this.slides;
        slides.forEach((slide, index) => {
          const offset = (index - this.activeIndex + slides.length) % slides.length;
          if (offset === 0) {
            slide.style.opacity = '1';
            slide.style.transform = 'scale(1)';
          } else if (offset === 1) {
            slide.style.transform = 'scale(1)';
          } else if (offset === 2) {
            slide.style.transform = 'scale(1)';
          } else {
            slide.style.opacity = '0';
            slide.style.transform = 'scale(0.85)';
          }
        });
      }
    },
    breakpoints: {
      320: {
        spaceBetween: 16,
        allowTouchMove: false
      },
      750: {
        spaceBetween: 20,
        allowTouchMove: true
      },
      768: {
        spaceBetween: 14,
      },
      1280: {
        spaceBetween: 20,
      },
      1440: {
        spaceBetween: 24,
      }
    }

  });
</script></body></html>