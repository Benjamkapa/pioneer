<script>
    function popupVideo() {
      const play = document.querySelector('.video-wrapper');
      const closePopup = document.querySelector('.btn-close-popup');
      const videoBg = document.querySelector('.popup-bg');
      const popup = document.querySelector('.case-popup');

      if (!play || !closePopup || !videoBg || !popup) {
        console.error('One or more elements are missing');
        return;
      }

      let tag = document.createElement('script');
      tag.src = "https://www.youtube.com/iframe_api";
      let firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

      let player;
      window.onYouTubeIframeAPIReady = function() {
        player = new YT.Player('player', {
          videoId: '',
          playerVars: {
            'playsinline': 1
          },
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        });
      }

      function onPlayerReady(event) {
        play.addEventListener('click', () => {
          event.target.playVideo();
          popup.style.display = 'block';
          setTimeout(() => {
            popup.style.opacity = 1;
          }, 0);
        });
      }

      function closeHandler() {
        popup.style.opacity = 0;
        setTimeout(() => {
          popup.style.display = 'none';
        }, 500);
        stopVideo();
      }

      let done = false;
      function onPlayerStateChange(event) {
        if (event.data == YT.PlayerState.PLAYING && !done) {
          closePopup.addEventListener('click', () => {
            closeHandler();
          });

          videoBg.addEventListener('click', () => {
            closeHandler();
          });

          done = true;
        }
      }

      function stopVideo() {
        player.stopVideo();
      }

      document.addEventListener('keydown', (event) => {
        if (event.key === 'Escape') {
          closeHandler();
        }
      });
    }

    document.addEventListener('DOMContentLoaded', (event) => {
      popupVideo();
    });
  </script>

<script defer>
if($(window).width() > 991){
const images = Array.from(document.querySelectorAll(".case-reach-text-block figure img"));
images.forEach(img => {
  mediumZoom(img, {
    margin: 0,
    background: "#fff",
    scrollOffset: 40,
    template: null,
    container: {
      	top: 50,
        bottom: 50,
      },
  });
  });
};
if($(window).width() <= 991){
const images = Array.from(document.querySelectorAll(".case-reach-text-block figure img"));
images.forEach(img => {
  mediumZoom(img, {
    margin: 0, 
    background: "#fff",
    scrollOffset: 0, 
    container: null,
    template: null
  });
  });
};

function adjustStyles() {
	const caseReachTextBlock = document.querySelector('.case-reach-text-block');
  if (!caseReachTextBlock) return;

  const childElements = Array.from(caseReachTextBlock.children);

  childElements.forEach(element => {
    const caseBrandSection = element.querySelector('.case-brand');

    if (element.tagName === 'H2' && element === caseReachTextBlock.firstElementChild) {
      if (caseBrandSection) {
        caseBrandSection.style.paddingBottom = '0';
      }
    }

    if (element.tagName === 'H2') {
      const previousSibling = element.previousElementSibling;
      if (previousSibling && (previousSibling.tagName === 'FIGURE' || previousSibling.tagName === 'IMG')) {
        element.style.marginTop = '0';
      } else {
        element.style.marginTop = '';
      }
    }

    const wEmbed = element.classList.contains('w-embed') ? element : null;
    if (wEmbed) {
      if (wEmbed.nextElementSibling && wEmbed.nextElementSibling.tagName === 'FIGURE') {
        const testimonial = wEmbed.querySelector('.testimonial');
        if (testimonial) {
          testimonial.style.marginBottom = '0';
        }
      }

      if (wEmbed.previousElementSibling && wEmbed.previousElementSibling.tagName === 'FIGURE') {
        const testimonial = wEmbed.querySelector('.testimonial');
        if (testimonial) {
          testimonial.style.marginTop = '0';
        }
      }

      if (wEmbed.nextElementSibling && wEmbed.nextElementSibling.tagName === 'H2') {
        const testimonial2 = wEmbed.querySelector('.testimonial');
        if (testimonial2) {
          testimonial2.style.marginBottom = '0';
        }
      }
    }
  });


  if (caseReachTextBlock && caseReachTextBlock.firstElementChild.tagName === 'H2') {
    const caseBrandElements = document.querySelectorAll('.case-brand');

    caseBrandElements.forEach(element => {
      element.style.paddingBottom = '0';
    });
  }
}

adjustStyles() 

function toUppercase() {
    const figureImage = document.querySelectorAll('figure');

      figureImage.forEach(item => {
        const figcaption = item.querySelector('figcaption');

        if (figcaption) {
          const textFigcaption = figcaption.textContent.toLocaleLowerCase();
          if (textFigcaption === 'before' || textFigcaption === 'after') {
            figcaption.style.textTransform = 'uppercase';
          }
        }
   });
 }
 
toUppercase();
</script>

<script defer>
function videoPlayer() {
    const videoContainers = document.querySelectorAll('.video-wrap');
    let currentlyPlayingVideo = null;

    videoContainers.forEach(container => {
        const video = container.querySelector('.video');
        const playButton = container.querySelector(".btn-play");
        const pauseButton = container.querySelector(".btn-pause");
        const videoOverlay = container.querySelector(".video-overlay");

        let isFirstClick = true;
        let hideTimeout;

        playButton.style.display = "block";
        pauseButton.style.display = "none";
        
        video.setAttribute("controlslist", "nodownload");


        function togglePlay() {
            if (currentlyPlayingVideo && currentlyPlayingVideo !== video) {
                currentlyPlayingVideo.pause();
                currentlyPlayingVideo.parentElement.querySelector('.btn-play').style.display = 'block';
                currentlyPlayingVideo.parentElement.querySelector('.btn-pause').style.display = 'none';
            }

            if (isFirstClick) {
                video.currentTime = 0;
                video.muted = false;
                video.play();
                isFirstClick = false;
                currentlyPlayingVideo = video;
                hideOverlay();
            } else {
                if (video.paused || video.ended) {
                    video.muted = false;
                    video.play();
                    currentlyPlayingVideo = video;
                } else {
                    video.pause();
                }
            }

            updateButtons();
        }

        function updateButtons() {
            if (video.paused || video.ended) {
                playButton.style.display = "block";
                pauseButton.style.display = "none";
            } else {
                playButton.style.display = "none";
                pauseButton.style.display = "block";
            }
        }

        function showButton() {
            if (!video.paused && !isFirstClick) {
                pauseButton.style.display = "block";
            }
            clearTimeout(hideTimeout);
            hideTimeout = setTimeout(() => {
                if (!video.paused && !isFirstClick && !pauseButton.matches(':hover')) {
                    pauseButton.style.display = "none";
                }
            }, 2500);
        }

        function hideButton(e) {
            if (!container.contains(e.relatedTarget) && !video.paused && !isFirstClick) {
                pauseButton.style.display = "none";
            }
        }

        function resetHideTimeout(e) {
            if (!pauseButton.matches(':hover')) {
                showButton();
            }
        }

        function nativeControlListener() {
            if (!isFirstClick) {
                updateButtons();
                showButton();
            }
        }

        function hideOverlay() {
            if (videoOverlay) {
                videoOverlay.style.display = 'none';
            }
        }

        playButton.addEventListener("click", togglePlay);
        pauseButton.addEventListener("click", togglePlay);

        video.addEventListener("play", nativeControlListener);
        video.addEventListener("pause", nativeControlListener);
        video.addEventListener("ended", nativeControlListener);

        container.addEventListener("mouseover", showButton);
        container.addEventListener("mouseout", hideButton);
        container.addEventListener("mousemove", resetHideTimeout);

        pauseButton.addEventListener("mouseover", showButton);
        pauseButton.addEventListener("mouseout", hideButton);
    });
}


  document.addEventListener("DOMContentLoaded", () => {
    videoPlayer();
  });
 </script></body></html>