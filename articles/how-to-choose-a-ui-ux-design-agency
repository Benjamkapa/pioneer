<script>

	const changeTextLink = () => {
  const link = document.querySelector('.art-socials-link--copy');
  const textLinkWrap = document.querySelector('.art-socilas-link-img-wrap--out-link');
  const textLink = document.querySelector('.art-socials-text-copy');

  let span = document.createElement('span');

  span.style.opacity = 0;
  span.style.transition = 'all 0.3s ease';
  span.textContent = 'copied!';

  link.addEventListener('click', (e) => {
    e.preventDefault();
    textLink.style.opacity = 0;
    textLinkWrap.append(span)
    setTimeout(()=> {
      textLink.style.display = 'none';
      textLinkWrap.style.backgroundColor = '#d0abff'
      span.style.opacity = 1;
    })
  }, 500);
}

$(".art-socials-link--copy").click(function(event) {
    event.preventDefault();
    navigator.clipboard.writeText($(this).attr("href"));
});


changeTextLink();

animateDiv('.art-hero-anim-1');
animateDiv('.art-hero-anim-2');
animateDiv('.art-hero-anim-3');
animateDiv('.art-hero-anim-4');
animateDiv('.art-hero-anim-5');

function makeNewPosition() {
  const h = $('.container').height() - 90;
  const w = $('.container').width() - 90;

  const nh = Math.floor(Math.random() * h);
  const nw = Math.floor(Math.random() * w);

  return [nh, nw];
}

function animateDiv(myclass){
    const newq = makeNewPosition();
    
    $(myclass).animate({ top: newq[0], left: newq[1] }, 2000,   function(){
      animateDiv(myclass);        
    });  
};

  
// "use strict"; // fix lenis in safari
jQuery(function($) {
  $('.art-hero-anim').mouseover(function() {
      const dWidth = $('.art-header').width() - 50, // 100 = image width
            dHeight = $('.art-header').height() - 50, // 100 = image height
            nextX = Math.floor(Math.random() * dWidth),
            nextY = Math.floor(Math.random() * dHeight);
      $(this).animate({ left: nextX + 'px', top: nextY + 'px' });
  });
});

class Scroll extends Lenis {
  constructor() {
    super({
      duration: 2,
      easing: (t) => (t === 1 ? 1 : 1 - Math.pow(2, -10 * t)), // https://easings.net
      direction: "vertical",
      smooth: true,
      smoothTouch: false,
      touchMultiplier: 1.5
    });

    this.time = 0;
    this.isActive = true;
    this.init();
  }

  init() {
    this.config();
    this.render();
    this.handleEditorView();
  }

  config() {
    // allow scrolling on overflow elements
    const overscroll = [
      ...document.querySelectorAll('[data-scroll="overscroll"]')
    ];

    if (overscroll.length > 0) {
      overscroll.forEach((item) =>
        item.setAttribute("onwheel", "event.stopPropagation()")
      );
    }

    // stop and start scroll btns
    const stop = [...document.querySelectorAll('[data-scroll="stop"]')];
    if (stop.length > 0) {
      stop.forEach((item) => {
        item.onclick = () => {
          this.stop();
          this.isActive = false;
        };
      });
    }

    const start = [...document.querySelectorAll('[data-scroll="start"]')];
    if (start.length > 0) {
      start.forEach((item) => {
        item.onclick = () => {
          this.start();
          this.isActive = true;
        };
      });
    }

    // toggle page scrolling
    const toggle = [...document.querySelectorAll('[data-scroll="toggle"]')];
    if (toggle.length > 0) {
      toggle.forEach((item) => {
        item.onclick = () => {
          if (this.isActive) {
            this.stop();
            this.isActive = false;
          } else {
            this.start();
            this.isActive = true;
          }
        };
      });
    }

    // anchor links
    const anchor = [...document.querySelectorAll("[data-scrolllink]")];
    if (anchor.length > 0) {
      anchor.forEach((item) => {
        const id = parseFloat(item.dataset.scrolllink);
        const target = document.querySelector(`[data-scrolltarget="${id}"]`);
        if (target) {
          //console.log(id, target);
          item.onclick = () => this.scrollTo(target);
        }
      });
    }
  }

  render() {
    this.raf((this.time += 10));
    window.requestAnimationFrame(this.render.bind(this));
  }

  /* ---- */
  handleEditorView() {
    const html = document.documentElement;
    const config = { attributes: true, childList: false, subtree: false };

    const callback = (mutationList, observer) => {
      for (const mutation of mutationList) {
        if (mutation.type === "attributes") {
          const btn = document.querySelector(".w-editor-bem-EditSiteButton");
          const bar = document.querySelector(".w-editor-bem-EditorMainMenu");
          const addTrig = (target) =>
            target.addEventListener("click", () => this.destroy());

          if (btn) addTrig(btn);
          if (bar) addTrig(bar);
        }
      }
    };

    const observer = new MutationObserver(callback);
    observer.observe(html, config);
  }
}

window.SmoothScroll = new Scroll();
</script>

 <script>
      const play = document.querySelector('.video-wrapper');
      const pause = document.querySelector('.btn-close-popup');

      let tag = document.createElement('script');
      tag.src = "https://www.youtube.com/iframe_api";
      let firstScriptTag = document.getElementsByTagName('script')[0];
      firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

      let player;
      function onYouTubeIframeAPIReady() {
        player = new YT.Player('player', {
          videoId: 'g3YevDztMiU',
          playerVars: {
            'playsinline': 1
          },
          events: {
            'onReady': onPlayerReady,
            'onStateChange': onPlayerStateChange
          }
        });
      }

      // 4. The API will call this function when the video player is ready.
      function onPlayerReady(event) {
        play.addEventListener('click', () => {
          event.target.playVideo();
        })
      }

      let done = false;
      function onPlayerStateChange(event) {
        if (event.data == YT.PlayerState.PLAYING && !done) {
          pause.addEventListener('click', () => {
          	setTimeout(() => {
            	stopVideo();
						}, 100);
          })

          done = true;
        }
      }

      function stopVideo() {
        player.stopVideo();
      }
    </script></body></html>