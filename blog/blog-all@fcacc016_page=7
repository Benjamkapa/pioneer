<script defer type="text/javascript">

function addCheckboxListener() {
  const checkboxFS = document.querySelectorAll(".w-checkbox-input");
  checkboxFS.forEach((el) => {
    const label = el.parentNode;
    el.addEventListener("change", function () {
      if (el.checked) {
        label.classList.add('fs-cc-active-checkbox');
      } else {
        label.classList.remove('fs-cc-active-checkbox');
      }
    });
  });
}

addCheckboxListener();

  function removeTransformOnResize() {
    const nav = document.querySelector('.main-header__nav');
    const screenWidth = window.innerWidth;

    if (screenWidth >= 768) {
      nav.style.transform = '';
    } else {
      nav.style.transform = 'translateY(0px) translateX(0px)';
    }
  }

  window.addEventListener('resize', removeTransformOnResize);

  const headers = document.querySelectorAll('.main-header');
  let lastScrollPosition = 0;

  function handleScroll(elements) {
    const currentScrollPosition = window.pageYOffset;
    const scrollDifference = currentScrollPosition - lastScrollPosition;

    for (let i = 0; i < elements.length; i++) {
      const el = elements[i];

      if (currentScrollPosition > 50) {
        if (scrollDifference > 0) {
          el.style.opacity = '0';
          el.style.top = '-15%';
        } else {
          el.style.opacity = '1';
          el.style.top = '16px';
        }
      } else {
        el.style.opacity = '1';
        el.style.top = '16px';
      }
    }

    lastScrollPosition = currentScrollPosition;
  }

  window.addEventListener('scroll', function () {
    handleScroll(headers);
  });

  function btnToggle() {
    const btns = document.querySelectorAll('.btn-burger-wrap');

    btns.forEach(btn => {
      const line1 = btn.querySelector('.btn-burger-line-1');
      const line2 = btn.querySelector('.btn-burger-line-2');
      const line3 = btn.querySelector('.btn-burger-line-3');

      function toggleBtn() {
        line1.classList.toggle('btn-burger-line-1-active');
        line2.classList.toggle('btn-burger-line-2-active');
        line3.classList.toggle('btn-burger-line-3-active');
      }

      btn.addEventListener('click', toggleBtn);

      const mediaQuery = window.matchMedia('(min-width: 768px)');
      function handleTabletChange(e) {
        if (e.matches) {
          line1.classList.toggle('btn-burger-line-1-active', false);
          line2.classList.toggle('btn-burger-line-2-active', false);
          line3.classList.toggle('btn-burger-line-3-active', false);
        }
      }
      mediaQuery.addListener(handleTabletChange);
    });
  }

  btnToggle();
</script>

<script>
(function() {
  const params = new URLSearchParams(window.location.search);
  const gclid = params.get('gclid');
  
  if (gclid && gclid.length >= 20 && gclid.match(/^[a-zA-Z0-9_-]+$/)) {
    localStorage.setItem('gclid', gclid);
  }
})();

document.addEventListener("DOMContentLoaded", function() {
  const gclidFields = document.querySelectorAll('input[name="gclid"]');
  const savedGclid = localStorage.getItem('gclid') || '';
  
  gclidFields.forEach(function(field) {
    field.value = savedGclid;
  });
});
</script>
<script>
// Function to initialize the pagination scroll behavior
function initPaginationScroll() {
    const paginationButtonsWrapper = document.querySelector('.pagination-wrap-btns');
    const paginationButtonClassName = 'pagin-btn';

    const scrollTarget = document.querySelector('.blog-posts-all-wrap');
    const scrollOffset = 100;
    const scrollDelay = 500;

    paginationButtonsWrapper.addEventListener('click', function (e) {
        const buttonTypes = ['A'];

        setTimeout(function () {
            const buttonElement = e.target.closest('.' + paginationButtonClassName);
            if (
                buttonElement &&
                buttonTypes.includes(buttonElement.tagName)
            ) {
                const y = scrollTarget.getBoundingClientRect().top + window.scrollY;
                window.scroll({
                    top: y - scrollOffset,
                    behavior: 'smooth',
                });
            }
        }, scrollDelay); // The delay in milliseconds (half a second)
    });
}

// Function to observe the addition of <a> tags to .blog-posts-all-wrap
function observeLinksAddition() {
    const targetNode = document.querySelector('.blog-posts-all-wrap');
    const config = { childList: true, subtree: true };

    const callback = function (mutationsList, observer) {
        for (const mutation of mutationsList) {
            if (mutation.addedNodes.length > 0) {
                const addedLinks = Array.from(mutation.addedNodes).filter(node => node.tagName === 'A');
                if (addedLinks.length > 0) {
                    observer.disconnect(); // Stop observing once <a> tags are added
                    initPaginationScroll(); // Initialize the scroll behavior
                }
            }
        }
    };

    const observer = new MutationObserver(callback);
    observer.observe(targetNode, config);
}

// Start observing when the DOM is fully loaded
document.addEventListener('DOMContentLoaded', function() {
    observeLinksAddition();
});
</script>



<script async>
function addPaginationLinks() {
    const url = new URL(window.location.href);
    const searchParams = new URLSearchParams(url.search);

    const currentPage = parseInt(searchParams.get('page')) || 1;
    const totalPages = 21;
    
    if (currentPage > 1) {
        const prevLink = document.createElement("link");
        prevLink.rel = "prev";
        prevLink.href = `${url.origin}${url.pathname}?page=${currentPage - 1}`;
        document.head.appendChild(prevLink);
    }

    // Adding the 'next' link if the current page is less than the total number of pages
    if (currentPage < totalPages) {
        const nextLink = document.createElement("link");
        nextLink.rel = "next";
        nextLink.href = `${url.origin}${url.pathname}?page=${currentPage + 1}`;
        document.head.appendChild(nextLink);
    }
}

addPaginationLinks();
</script>






</body></html>