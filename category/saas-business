<script defer type="text/javascript">

function addCheckboxListener() {
  const checkboxFS = document.querySelectorAll(".w-checkbox-input");
  checkboxFS.forEach((el) => {
    const label = el.parentNode;
    el.addEventListener("change", function () {
      if (el.checked) {
        label.classList.add('fs-cc-active-checkbox');
      } else {
        label.classList.remove('fs-cc-active-checkbox');
      }
    });
  });
}

addCheckboxListener();

  function removeTransformOnResize() {
    const nav = document.querySelector('.main-header__nav');
    const screenWidth = window.innerWidth;

    if (screenWidth >= 768) {
      nav.style.transform = '';
    } else {
      nav.style.transform = 'translateY(0px) translateX(0px)';
    }
  }

  window.addEventListener('resize', removeTransformOnResize);

  const headers = document.querySelectorAll('.main-header');
  let lastScrollPosition = 0;

  function handleScroll(elements) {
    const currentScrollPosition = window.pageYOffset;
    const scrollDifference = currentScrollPosition - lastScrollPosition;

    for (let i = 0; i < elements.length; i++) {
      const el = elements[i];

      if (currentScrollPosition > 50) {
        if (scrollDifference > 0) {
          el.style.opacity = '0';
          el.style.top = '-15%';
        } else {
          el.style.opacity = '1';
          el.style.top = '16px';
        }
      } else {
        el.style.opacity = '1';
        el.style.top = '16px';
      }
    }

    lastScrollPosition = currentScrollPosition;
  }

  window.addEventListener('scroll', function () {
    handleScroll(headers);
  });

  function btnToggle() {
    const btns = document.querySelectorAll('.btn-burger-wrap');

    btns.forEach(btn => {
      const line1 = btn.querySelector('.btn-burger-line-1');
      const line2 = btn.querySelector('.btn-burger-line-2');
      const line3 = btn.querySelector('.btn-burger-line-3');

      function toggleBtn() {
        line1.classList.toggle('btn-burger-line-1-active');
        line2.classList.toggle('btn-burger-line-2-active');
        line3.classList.toggle('btn-burger-line-3-active');
      }

      btn.addEventListener('click', toggleBtn);

      const mediaQuery = window.matchMedia('(min-width: 768px)');
      function handleTabletChange(e) {
        if (e.matches) {
          line1.classList.toggle('btn-burger-line-1-active', false);
          line2.classList.toggle('btn-burger-line-2-active', false);
          line3.classList.toggle('btn-burger-line-3-active', false);
        }
      }
      mediaQuery.addListener(handleTabletChange);
    });
  }

  btnToggle();
</script>

<script>
(function() {
  const params = new URLSearchParams(window.location.search);
  const gclid = params.get('gclid');
  
  if (gclid && gclid.length >= 20 && gclid.match(/^[a-zA-Z0-9_-]+$/)) {
    localStorage.setItem('gclid', gclid);
  }
})();

document.addEventListener("DOMContentLoaded", function() {
  const gclidFields = document.querySelectorAll('input[name="gclid"]');
  const savedGclid = localStorage.getItem('gclid') || '';
  
  gclidFields.forEach(function(field) {
    field.value = savedGclid;
  });
});
</script>
 <script>
    document.addEventListener('DOMContentLoaded', function () {
    	handleAccordionToggleAndInteraction();
      coloredBox('.blog-card-link', '.blog-card-category');
      makeScrollNavigationActive('.blog-navigation-wrap', 50, 'nav-wrap-active');
      positionDropdownForMobile();
    });
    
    
     function handleAccordionToggleAndInteraction() {
    const accordionToggles = document.querySelectorAll('.accordion-blog-toggle');
    const dropdowns = document.querySelectorAll('.accordion-blog-dropdown');
    const accordionLinks = document.querySelectorAll('.accordion-blog-link');

    accordionToggles.forEach(toggle => {
        toggle.addEventListener('click', function () {
            const dropdown = toggle.nextElementSibling;
            const icon = toggle.querySelector('.accordion-icon');

            if (dropdown.classList.contains('show')) {
                dropdown.classList.remove('show');
                setTimeout(() => {
                    dropdown.style.display = 'none'; 
                }, 200);
                if (icon) {
                    icon.style.transform = ''; 
                }
            } else {
                dropdown.style.display = 'block'; 
                setTimeout(() => {
                    dropdown.classList.add('show');
                }, 10);
                if (icon) {
                    icon.style.transform = 'rotateZ(180deg)';
                }
            }
        });
    });

    window.addEventListener('scroll', function () {
        dropdowns.forEach(dropdown => {
            if (dropdown.classList.contains('show')) {
                dropdown.classList.remove('show');
                setTimeout(() => {
                    dropdown.style.display = 'none';
                }, 200);
            }
        });

        accordionToggles.forEach(toggle => {
            const icon = toggle.querySelector('.accordion-icon');
            if (icon) {
                icon.style.transform = '';
            }
        });
    });

    accordionLinks.forEach(link => {
        link.addEventListener('click', function (event) {
            event.preventDefault();

            const linkText = this.querySelector('p').textContent.trim();
            const accordionWrapper = this.closest('.accordion-blog-wrapper');
            const titleTextElement = accordionWrapper.querySelector('.accordion-title-text');

            if (titleTextElement) {
                titleTextElement.textContent =  `saas business: ${linkText}`;
            }

            const dropdown = this.closest('.accordion-blog-dropdown');
            if (dropdown) {
                dropdown.classList.remove('show');
                setTimeout(() => {
                    dropdown.style.display = 'none';
                }, 200);

                const toggle = dropdown.previousElementSibling;
                if (toggle) {
                    const icon = toggle.querySelector('.accordion-icon');
                    if (icon) {
                        icon.style.transform = '';
                    }
                }
            }
        });
    });
}
    

 
    function coloredBox(cardItem, textCatagory) {
      const cards = document.querySelectorAll(cardItem);
      const colorMap = {
        'Design process': '#EDCC88',
        'SaaS business': '#E8DBCC',
        'Product design': '#C5A2BF',
        'Design team': '#EBB29A'
      };

      cards.forEach(function (card) {
        const text = card.querySelector(textCatagory);
        const content = text.textContent;

        if (content in colorMap) {
          text.style.backgroundColor = colorMap[content];
        }
      });
    }

    function makeScrollNavigationActive(selector, threshold, activeClass) {
      const elements = document.querySelectorAll(selector);
      let previousScrollPosition = window.pageYOffset;

      function onScroll() {
        const currentScrollPosition = window.pageYOffset;

        elements.forEach(el => {
          if (currentScrollPosition > threshold) {
            if (currentScrollPosition > previousScrollPosition) {
              el.classList.add(activeClass);
            } else {
              el.classList.remove(activeClass);
            }
          } else {
            el.classList.remove(activeClass);
          }
        });

        previousScrollPosition = currentScrollPosition;
      }

      window.addEventListener('scroll', onScroll);
    }


    function positionDropdownForMobile() {
      const accordionToggle = document.querySelector('.accordion-blog-toggle');
      const accordionDropdown = document.querySelector('.accordion-blog-dropdown');

      if (window.innerWidth <= 768) {
        const rect = accordionToggle.getBoundingClientRect();
        accordionDropdown.style.top = `${rect.bottom}px`;
      } else {
        accordionDropdown.style.top = '';
      }
    }




    window.addEventListener('resize', positionDropdownForMobile);

    document.querySelector('.accordion-blog-toggle').addEventListener('click', function (e) {
      positionDropdownForMobile();
    });
  </script></body></html>