<script>
  Webflow.push(function () {
    const sliders = document.querySelectorAll('.slider-cards');

    sliders.forEach(function (slider) {
     
      if (slider.classList.contains('with-arrows')) {
        const prevArrow = slider.querySelector('.slider-cards-prev');
        const nextArrow = slider.querySelector('.slider-cards-next');

        if (typeof Swiper !== 'undefined' && slider) {
          new Swiper(slider, {
            slidesPerView: 1.05,
            spaceBetween: 16,
            pagination: {
              el: '.swiper-pagination',
              clickable: true,
            },
            navigation: {
              prevEl: prevArrow || undefined,
              nextEl: nextArrow || undefined,
            },
            breakpoints: {
              500: {
                slidesPerView: 2,
                spaceBetween: 16,
              },
              768: {
                slidesPerView: 2,
                spaceBetween: 16,
              },
              1280: {
                slidesPerView: 2,
                spaceBetween: 24,
              },
            },
          });
        }
      }
      else {
        if (typeof Swiper !== 'undefined' && slider) {
          new Swiper(slider, {
            slidesPerView: 1.05,
            spaceBetween: 16,
            autoHeight: true,
            pagination: {
              el: '.swiper-pagination',
              clickable: true,
            },
            breakpoints: {
              500: {
                slidesPerView: 2,
                spaceBetween: 16,
                autoHeight: true,
              },
              768: {
                slidesPerView: 2,
                spaceBetween: 16,
                autoHeight: true,
              },
              1280: {
                slidesPerView: 2,
                spaceBetween: 24,
                autoHeight: true,
              },
            },
          });
        }
      }
    });
  });
</script>



<script>
  var b = document.querySelector("html");

  b.setAttribute("itemtype", "https://schema.org/FAQPage");
  b.setAttribute("itemscope", "itemscope");

  $(document).ready(function () {
    var match = document.cookie.match('(?:^|;)\\s*_ga=([^;]*)');
    var raw = (match) ? decodeURIComponent(match[1]) : null;
    var input = document.getElementById("customerid");
    if (raw) {
      match = raw.match(/(\d+\.\d+)$/);
    }
    var gacid = (match) ? match[1] : null;
    if (gacid) {
      input.value = gacid;
    }
  });

  var $form = $("form");
  
  $.validator.addMethod("letters", function (value, element) {
    return this.optional(element) || value == value.match(/^[a-zA-Z\s]*$/);
  });

  $form.validate({
    rules: {
      contactname: {
        required: true,
        minlength: 2,
        letters: true
      },
      canwehelp: {
        required: true,
        minlength: 3,
        letters: false
      },
      youremail: {
        required: true,
        email: true
      },
      wherehear: {
        required: true,
      }
    },
    messages: {
      contactname: "Please specify your name",
      youremail: "Please specify a valid email address",
      canwehelp: "Please tell us about your product",
      wherehear: "Please specify how did you hear about us"
    }
  });

  $(document).ready(function () {
    $('select').niceSelect();
  });
</script></body></html>